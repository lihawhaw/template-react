{
  "name": "template-react",
  "version": "1.0.0",
  "description": "React 模板",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start:dashboard": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:speed": "cross-env NODE_ENV=production SPEED=true webpack --progress",
    "lint": "eslint ./src --ext .js,.ts,.tsx --cache --fix",
    "format": "prettier --write --cache .",
    "fix-code": "prettier-eslint --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "analyzer": "cross-env NODE_ENV=production ANALYZER=true webpack",
    "webpack:stats": "webpack --config webpack.config.js --profile --json > stats.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "lihawhaw@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@swc/core": "^1.4.11",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/loadable__component": "^5.13.8",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/react-test-renderer": "^18.0.7",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^1.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "esbuild-loader": "^4.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "happy-dom": "^14.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.8.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.4",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.2",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
