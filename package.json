{
  "name": "template-react",
  "version": "1.0.0",
  "description": "React 模板",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start:dashboard": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:speed": "cross-env NODE_ENV=production SPEED=true webpack --progress",
    "lint": "eslint ./src --ext .js,.ts,.tsx --cache --fix",
    "format": "prettier --write --cache .",
    "fix-code": "prettier-eslint --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "analyzer": "cross-env NODE_ENV=production ANALYZER=true webpack",
    "webpack:stats": "webpack --config webpack.config.js --profile --json > stats.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "lihawhaw@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@swc/core": "^1.3.76",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-transition-group": "^4.4.7",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "esbuild-loader": "^4.0.2",
    "eslint": "8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "happy-dom": "^12.9.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
