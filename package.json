{
  "name": "template-react",
  "version": "1.0.0",
  "description": "React 模板",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:speed": "cross-env NODE_ENV=production SPEED=true webpack --progress",
    "lint": "eslint ./src --ext .js,.ts,.tsx --cache --fix",
    "format": "prettier --write --cache .",
    "fix-code": "prettier-eslint --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "analyzer": "cross-env NODE_ENV=production ANALYZER=true webpack",
    "webpack:stats": "webpack --config webpack.config.js --profile --json > stats.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "lihawhaw@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.35",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^3.0.1",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "happy-dom": "^8.3.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
